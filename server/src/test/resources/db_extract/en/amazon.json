                                    {
                                        "sense_id": "a731184e-3d6a-42e3-aa95-8533bc4c98e1",
                                        "entry_id": "dd4259ae-a95f-40d1-b154-461304b223f5",
                                        "glosses": [
                                            "представительница племени воительниц"
